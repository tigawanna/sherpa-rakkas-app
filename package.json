{
  "name": "sherpa-rakkas-app",
  "type": "module",
  "private": true,
  
  "scripts": {
    "dev": "rakkas",
    "build": "rakkas build",
    "start": "node dist/server",
    "format": "prettier --write --ignore-unknown src",
    "test": "pnpm test:typecheck && pnpm test:format && pnpm test:lint && pnpm test:unit",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist",
    "test:unit": "vitest run",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && rakkas build",
    "prod-build": "prisma generate && prisma migrate deploy && rakkas build",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "@prisma/client": "5.4.1",
    "@rakkasjs/eslint-config": "0.7.0-next.21",
    "@tailwindcss/typography": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "10.4.15",
    "daisify-shadcn": "^0.0.1",
    "daisyui": "3.5.1",
    "eslint": "^8.34.0",
    "postcss": "8.4.27",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.4.1",
    "prisma": "5.4.1",
    "rakkasjs": "0.7.0-next.21",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.6",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5",
    "zod-prisma-types": "^2.7.9"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^1.1.0",
    "@hattip/cookie": "^0.0.35",
    "@hattip/response": "^0.0.35",
    "@hookform/resolvers": "^3.3.1",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@lucia-auth/oauth": "^3.3.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanem/react-nprogress": "^5.0.51",
    "@unpic/react": "^0.0.35",
    "cherry-markdown": "^0.8.26",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "google-auth-library": "^9.1.0",
    "lucia": "^2.7.1",
    "lucide-react": "^0.279.0",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.8.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-select": "^5.7.7",
    "react-toastify": "^9.1.3",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.3"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
